<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="468.0" prefWidth="617.0" stylesheets="@../css/style.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.CustomerReportController">
   <children>
      <Label layoutX="155.0" layoutY="14.0" text="Number of Customers by Country and Division">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <Button layoutX="560.0" layoutY="429.0" mnemonicParsing="false" onAction="#exitButton" text="Exit" />
      <TableView fx:id="countryTable" layoutX="21.0" layoutY="45.0" prefHeight="379.0" prefWidth="276.0">
         <columns>
            <TableColumn fx:id="countryColumn" prefWidth="144.0" text="Country" />
            <TableColumn fx:id="numCountryColumn" minWidth="0.0" prefWidth="130.0" text="Number of Customers" />
         </columns>
      </TableView>
      <TableView fx:id="divisionTable" layoutX="322.0" layoutY="45.0" prefHeight="379.0" prefWidth="276.0">
         <columns>
            <TableColumn fx:id="divisionColumn" prefWidth="144.0" text="Division" />
            <TableColumn fx:id="numDivColumn" minWidth="0.0" prefWidth="130.0" text="Number of Customers" />
         </columns>
      </TableView>
   </children>
</AnchorPane>
